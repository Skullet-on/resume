<p id="notice"><%= notice %></p>

<h1>Listing Educations</h1>

<table>
  <thead>
    <tr>
      <th>Organization</th>
      <th>Qualification</th>
      <th>Major</th>
      <th>Other</th>
      <th>Date start</th>
      <th>Date finish</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @educations.each do |education| %>
      <tr>
        <td><%= education.organization %></td>
        <td><%= education.qualification %></td>
        <td><%= education.major %></td>
        <td><%= education.other %></td>
        <td><%= education.date_start %></td>
        <td><%= education.date_finish %></td>
        <td><%= link_to 'Show', education %></td>
        <td><%= link_to 'Edit', edit_education_path(education) %></td>
        <td><%= link_to 'Destroy', education, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Education', new_education_path %>
