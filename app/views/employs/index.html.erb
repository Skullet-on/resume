<p id="notice"><%= notice %></p>

<h1>Listing Employs</h1>

<table>
  <thead>
    <tr>
      <th>Organization</th>
      <th>Position</th>
      <th>Responsibility</th>
      <th>Date start</th>
      <th>Date finish</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employs.each do |employ| %>
      <tr>
        <td><%= employ.organization %></td>
        <td><%= employ.position %></td>
        <td><%= employ.responsibility %></td>
        <td><%= employ.date_start %></td>
        <td><%= employ.date_finish %></td>
        <td><%= link_to 'Show', employ %></td>
        <td><%= link_to 'Edit', edit_employ_path(employ) %></td>
        <td><%= link_to 'Destroy', employ, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employ', new_employ_path %>
